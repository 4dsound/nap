{
	"Objects":
	[
		{
			"Type" : "nap::audio::AudioInterface",
			"mID": "MyAudioInterface",
			"UseDefaultDevice": "True",
			"SampleRate": "48000",
			"BufferSize": "256",
			"AllowFailure": "False"
		},

		{
			"Type": "nap::audio::AudioFileResource",
			"mID": "MyAudioFile",
			"AudioFilePath": "data/audiotest/test.wav",
			"AllowFailure": "False"
		},

		{
			"Type" : "nap::Entity",
			"mID": "AudioPlayerEntity",
			"Components" : 
			[
				{
					"Type": "nap::audio::PlayerComponent",
					"mID": "MyPlayer",
					"AudioInterface": "MyAudioInterface",
					"AudioFile": "MyAudioFile"
				},
				{
					"Type": "nap::audio::StereoPannerComponent",
					"mID": "MyPanner",
					"AudioInterface": "MyAudioInterface",
					"Input": "MyPlayer",
					"Panner": "0.1"
				},
				{
					"Type": "nap::audio::OutputComponent",
					"AudioInterface": "MyAudioInterface",
					"Input": "MyPanner"
				}
			]
		}
	]
}