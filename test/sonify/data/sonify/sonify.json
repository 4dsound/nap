{
	"Objects":
	[


		{
			"Type": "nap::OSCReceiver",
			"mID": "oscReceiver",
			"Port": 7000,
			"EnableDebugOutput": "True"
		},

		{
			"Type" : "nap::Image",
			"mID" : "image",
			"ImagePath" : "data/sonify/attractor.jpeg",
			"Parameters" : {
				"MaxFilter" : "Linear",		
				"MinFilter" : "LinearMipmapLinear",		
				"WrapVertical" : "ClampToEdge",	
				"WrapHorizontal" : "ClampToEdge",
				"MaxLodLevel" : 3
			}
		},	

		{
			"Type": "nap::audio::Voice",
			"mID": "voice",
			"Objects":
			[
				{
					"Type": "nap::audio::Oscillator",
					"mID": "oscillator",
					"Amplitude": [ 1 ],
					"ChannelCount": 1
				},

				{
					"Type": "nap::audio::Envelope",
					"mID": "envelope",
					"Envelope":
					[
						{
							"Duration": "1",
							"Destination": "1",
							"DurationRelative": "True"
						},
						{
							"Duration": "1",
							"Destination": "1",
							"DurationRelative": "True"
						},
						{
							"Duration": "1",
							"Destination": "0",
							"DurationRelative": "True"
						}
					]
				},

				{
					"Type": "nap::audio::Gain",
					"mID": "gain",
					"Inputs": [ "oscillator", "envelope" ],
					"Gain": [ 0.5 ],
					"ChannelCount": 1
				}
			],

			"Output": "gain",
			"Envelope": "envelope"
		},

		{
			"Type": "nap::Entity",
			"mID": "audioEntity",
			"Components":
			[
				{					
					"Type": "nap::audio::AudioComponent",
					"mID": "audioComponent",
					"Object":
					{
						"Type": "nap::audio::PolyphonicObject",
						"Voice": "voice",
						"VoiceCount": 32
					}
				},

				{
					"Type": "nap::audio::OutputComponent",
					"mID": "output",
					"Routing": [ 0, 0 ],
					"Input": "audioComponent"
				},

				{
					"Type": "nap::ImageReaderComponent",
					"mID": "imageReader",
					"Image": "image"
				},

				{
					"Type": "nap::PythonScriptComponent",
					"mID": "script",
					"Path": "data/sonify/scripts/sonify.py"
				}

			]
		}
	]
}